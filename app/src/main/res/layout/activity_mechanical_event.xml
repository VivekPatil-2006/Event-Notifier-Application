<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".Mechanical_Event">

    <ImageView
        android:id="@+id/imageView51"
        android:layout_width="196dp"
        android:layout_height="195dp"
        android:layout_marginTop="28dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/car_event" />

    <ImageView
        android:id="@+id/imageView52"
        android:layout_width="189dp"
        android:layout_height="189dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView51"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/drama_event" />

    <ImageView
        android:id="@+id/imageView53"
        android:layout_width="196dp"
        android:layout_height="193dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView51"
        app:srcCompat="@drawable/maths_event" />
</androidx.constraintlayout.widget.ConstraintLayout>